<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="poliues.eisi.fia.edu.sv.poliues"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="poliues.eisi.fia.edu.sv.poliues.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="poliues.eisi.fia.edu.sv.poliues.principal"
            android:label="@string/title_activity_principal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="poliues.eisi.fia.edu.sv.poliues.SolicitudInsertarActivity" />
        <activity android:name="poliues.eisi.fia.edu.sv.poliues.DetalleSolicitudInsertarActivity" />
        <activity android:name="poliues.eisi.fia.edu.sv.poliues.SolicitudActualizarActivity" />
        <activity android:name="poliues.eisi.fia.edu.sv.poliues.SolicitudEliminarActivity" />
        <activity android:name="poliues.eisi.fia.edu.sv.poliues.SolicitudConsultarActivity" />
        <activity android:name="poliues.eisi.fia.edu.sv.poliues.DetalleSolicitudActualizarActivity" />
        <activity
            android:name="poliues.eisi.fia.edu.sv.poliues.AdministradorActivity"
            android:label="@string/title_activity_administrador"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="poliues.eisi.fia.edu.sv.poliues.AdministradorInsertarActivity"
            android:label="@string/title_activity_administrador_agregar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="poliues.eisi.fia.edu.sv.poliues.SolicitanteActivity"
            android:label="@string/title_activity_solicitante"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="poliues.eisi.fia.edu.sv.poliues.SolicitanteInsertarActivity"
            android:label="@string/action_solicitante_agregar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="poliues.eisi.fia.edu.sv.poliues.AdministradorEditarActivity"
            android:label="@string/title_activity_administrador_editar"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>